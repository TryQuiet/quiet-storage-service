    "node": "22.14.0",
    "npm": "10.9.2",
    "pnpm": "10.6.0"
  },
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2",
    "pnpm": "10.6.0"
  },
  "packageManager": "pnpm@10.6.0"
}
