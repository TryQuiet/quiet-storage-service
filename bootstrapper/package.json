  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TryQuiet/quiet-storage-service.git"
  },
  "scripts": {
    "install:deps": "pnpm i --dir --ignore-workspace --force",
    "install:deps:deployed": "yes | pnpm i --dir --ignore-workspace --frozen-lockfile --child-concurrency 10 --stream",
    "bootstrap-app": "pnpm run install:deps && node ./bootstrap.mjs",
    "bootstrap-app:deployed": "pnpm run install:deps:deployed && node ./bootstrap.mjs --deployed --copy-submodules"
  },
  "devDependencies": {
    "winston": "^3.17.0",
    "@commander-js/extra-typings": "13.1.0",
    "ansi-colors": "^4.1.3"
  },
